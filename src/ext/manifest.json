{
	"manifest_version": "ext-v1",
	"name": "Excalidraw",
	"version": "1.0.7",
	"short_name": "Excalidraw",
	"description": "Virtual hand-drawn style whiteboard.",
	"author": "EXT, Inc.",
	"homepage_url": "https://github.com/excalidraw/excalidraw",
	"icons": {
		"128": "icons/icon-128.png",
		"128-dark": "icons/icon-128-dark.png",
		"background": "icons/icon-background.png",
		"background-dark": "icons/icon-background-dark.png",
		"banner": "icons/icon-banner.png",
		"banner-dark": "icons/icon-banner-dark.png"
	},
	"scripts": [
		"main.js"
	],
	"modules": {
		"console": {
			"module_version": "1.0.1"
		},
		"runtime": {
			"module_version": "1.2.1"
		},
		"tabs": {
			"module_version": "1.1.0"
		},
		"windows": {
			"module_version": "1.1.0"
		},
		"webviews": {
			"module_version": "1.0.2"
		},
		"websessions": {
			"module_version": "1.0.1",
			"optional_permissions": [
				"create.persistent"
			]
		}
	}
}